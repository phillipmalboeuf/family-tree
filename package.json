{
	"name": "@phillipmalboeuf/directus-interface-family-tree",
	"description": "A Directus interface extension for visualizing family trees with parent-child and marriage relationships",
	"icon": "extension",
	"version": "1.1.0",
	"license": "MIT",
	"author": "Philip Malboeuf",
	"repository": {
		"type": "git",
		"url": "https://github.com/phillipmalboeuf/family-tree.git"
	},
	"homepage": "https://github.com/phillipmalboeuf/family-tree#readme",
	"bugs": {
		"url": "https://github.com/phillipmalboeuf/family-tree/issues"
	},
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-interface",
		"family-tree",
		"genealogy",
		"family",
		"tree"
	],
	"type": "module",
	"main": "dist/index.js",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"publishConfig": {
		"access": "public"
	},
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"prepublishOnly": "npm run build",
		"test": "vite",
		"test:build": "vite build"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "17.0.5",
		"@vitejs/plugin-vue": "^5.0.0",
		"typescript": "^5.9.3",
		"vite": "^5.0.0",
		"vue": "^3.5.27"
	}
}
